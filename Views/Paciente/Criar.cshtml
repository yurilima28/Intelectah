@model PacienteModel

@{
    ViewData["Title"] = "Cadastrar paciente";
}

<div class="text-center">
    <h1 class="display-4"> Cadastrar Paciente</h1>

</div>

<form asp-controller="Paciente" asp-action="Criar" method="post">

    @Html.Partial("Forms.cshtml")

    <button type="submit" class="btn btn-primary">Adicionar</button>
    <a class="btn btn-secondary " asp-action="Index" asp-controller="Paciente" role="button">Voltar</a>

</form>